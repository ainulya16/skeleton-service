{
  "name": "skeleton-service",
  "version": "0.1.0",
  "description": "Microservice Skeleton",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "test": "jest",
    "watch": "jest --watch",
    "test:newman": "jest tests/** --detectOpenHandles",
    "cover": "jest --coverage",
    "prebuild": "rimraf lib/",
    "build": "babel src --out-dir lib --copy-files",
    "lint": "eslint \"src/**/*.js\"",
    "gen-chart": "pipeline-utils gen-chart -n $npm_package_name -d $npm_package_description -v $npm_package_version",
    "gen-api-doc": "pipeline-utils gen-api-doc -s lib/index.js -o $API_DOC_OUTPUT"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Ainul",
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "delay": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.1.0",
    "jest-junit": "^6.3.0",
    "newman": "^4.4.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "supertest": "^3.4.2",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.3.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-query-parser": "^1.0.2",
    "getenv": "^0.7.0",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "validator": "^10.11.0",
    "whispir-node-sdk": "^2.1.1"
  }
}
